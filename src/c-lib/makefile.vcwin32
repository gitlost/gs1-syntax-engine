gs1encoders.lib: $(OBJS)
	-$(RM) $@
	$(AR) $(ARFLAGS) /out:$@ $(OBJS)

clean:
	-$(RM) *.obj
	-$(RM) syntax\*.obj
	-$(RM) gs1encoders.lib
